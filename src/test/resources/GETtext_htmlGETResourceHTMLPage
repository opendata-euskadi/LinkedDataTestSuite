$(document).ready(function(){	
    $('.gaien_zerrenda_02').hide().before('<a href="#" id="open-example1" class="ireki_gaiak">Erakutsi gai gehiago</a>');
	$('.gaien_zerrenda_02').after('<a href="#close-example1" id="close-example1" class="ireki_gaiak">Erakutsi gai gutxiago</a>');
	$('a#close-example1').hide();
	$('a#open-example1').click(function() {
		$('.gaien_zerrenda_02').slideDown(1000);
		$('a#open-example1').fadeOut(0);
		$('a#close-example1').fadeIn(1000);
		return false;
	});
	$('a#close-example1').click(function() {
		$('.gaien_zerrenda_02').slideUp(1000);
		$('a#open-example1').fadeIn(1000);
		$('a#close-example1').fadeOut(0);
		return false;
	});
});
</script>
